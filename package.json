{
  "name": "aws-appsync-demo",
  "version": "0.0.0",
  "description": "TODO",
  "main": ".js",
  "scripts": {
    "watch": "npm run html:src && /Users/mollie/code/miru/srccli.js -p public -w [ npm run watch:js -o public/bundle.js -r bytes.written ] -w [ npm run watch:css -o public/bundle.css ] -t public/index.html",
    "build": "npm run build:src && npm run uglify && npm run html",
    "html": "cp templates/index-production.html public/index.html",
    "html:src": "cp templates/index-src.html public/index.html",
    "build:src": "npm run build:js && npm run build:css && npm run html:src",
    "build:js": "browserify --bare -t [ babelify --presets [ env ] ] src/main.js -o public/bundle.js",
    "watch:js": "watchify --debug --verbose --bare -t [ babelify --presets [ env ] ] src/main.js -o public/bundle.js",
    "build:css": "stylus -u autoprefixer-stylus src/main.styl -o public/bundle.css",
    "watch:css": "stylus -w -u autoprefixer-stylus src/main.styl -o public/bundle.css",
    "uglify": "npm run uglify:js && npm run uglify:css",
    "uglify:js": "uglifyjs public/bundle.js -cmo public/bundle.min.js",
    "uglify:css": "uglifycss public/bundle.css --output public/bundle.min.css",
    "prepublish": "npm run build && npm test",
    "lint": "spacestandard src/**.js",
    "fixlint": "spacestandard --fix src/**.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    ""
  ],
