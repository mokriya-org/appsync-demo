  "bugs": {
    "url": "https://github.com/talmobi//issues",
    "email": "talmo.christian@gmail.com"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer-stylus": "~0.14.0",
    "babel-core": "~6.26.3",
    "babel-preset-env": "~1.7.0",
    "babelify": "~8.0.0",
    "browserify": "~16.2.2",
    "spacestandard": "~0.2.0",
    "stylus": "~0.54.5",
    "uglify-js": "~3.4.8",
    "uglifycss": "0.0.29",
    "watchify": "~3.11.0"
  }
}
